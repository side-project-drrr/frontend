{
    "name": "service",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build && yarn run copy-sw",
        "preview": "vite preview",
        "copy-sw": "cp ./src/webpush/sw.js dist/sw.js",
        "postinstall": "husky install",
        "lint:fix": "eslint --cache --fix './src/**/*.{ts,tsx}'",
        "prettier:fix": "prettier --cache --write './src/**/*.{ts,tsx}'",
        "test": "vitest --run --reporter verbose --globals",
        "test:ui": "vitest --ui",
        "test:watch": "vitest",
        "cp-env:production": "cp security/security-storage-front/.env.production ./.env.production",
        "cp-env:dev": "cp security/security-storage-front/.env ./.env.development.local",
        "start": "vite",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "cy:open": "cypress open"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/base": "^5.0.0-beta.26",
        "@mui/icons-material": "^5.15.3",
        "@mui/material": "^5.14.20",
        "@mui/x-date-pickers": "^6.19.7",
        "@tanstack/react-query": "^5.37.1",
        "@vitejs/plugin-vue": "latest",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recoil": "^0.7.7"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "8.0.8",
        "@storybook/addon-interactions": "8.0.8",
        "@storybook/addon-links": "8.0.8",
        "@storybook/addon-onboarding": "8.0.8",
        "@storybook/addon-styling": "^1.3.7",
        "@storybook/blocks": "8.0.8",
        "@storybook/builder-vite": "^8.0.8",
        "@storybook/react": "8.0.8",
        "@storybook/react-vite": "8.0.8",
        "@storybook/testing-library": "0.2.2",
        "@tanstack/eslint-plugin-query": "^5.35.6",
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "@vitest/ui": "^0.34.6",
        "autoprefixer": "^10.4.16",
        "axios": "^1.6.0",
        "cypress": "^13.8.1",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "eslint-plugin-storybook": "^0.6.13",
        "eslint-plugin-vitest-globals": "^1.4.0",
        "framer-motion": "^10.16.0",
        "husky": "^8.0.3",
        "identity-obj-proxy": "^3.0.0",
        "js-base64": "^3.7.6",
        "jsdom": "^22.1.0",
        "lint-staged": "^15.2.2",
        "msw": "^2.0.3",
        "postcss": "latest",
        "prettier": "^3.2.5",
        "react-datepicker": "^4.18.0",
        "react-error-boundary": "^4.0.13",
        "react-icons": "^4.11.0",
        "react-kakao-login": "^2.1.1",
        "react-router-dom": "^6.22.3",
        "storybook": "8.0.8",
        "tailwindcss": "^3.3.4",
        "typescript": "^5.4.5",
        "vite": "latest",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.34.6"
    },
    "msw": {
        "workerDirectory": "public"
    }
}
